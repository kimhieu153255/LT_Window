<Window x:Class="WPF1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="firstPage_Load">
    <Canvas>
        <Label Name="labelQuote" Content="Inspiring Quotes" FontSize="20" FontWeight="SemiBold" Canvas.Left="320" Canvas.Top="34"/>
        <Image Name="imgQuote" Width="200" Height="200" Canvas.Left="300" Canvas.Top="154"/>
        <TextBox x:Name="texQuote" Height="50" Width="484" Canvas.Left="158" Canvas.Top="84" VerticalContentAlignment="Center" FontSize="13" TextAlignment="Center"/>
        <Button x:Name="btn" Click="clickButton" Content="Show" Width="70" Height="35" Canvas.Left="673" Canvas.Top="87"/>
    </Canvas>
</Window>
